<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>12</LangVersion>
        <TargetFrameworks>net8.0;net9.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Jan Biehl</Authors>
        <Description>This is a simple TcpLibrary containing a TcpClient &amp; TcpServer. </Description>
        <RepositoryUrl>https://github.com/janbiehl/Jbi.TcpLib</RepositoryUrl>
        <PackageTags>dotnet tcp tcpclient tcpserver</PackageTags>
        <PackageReleaseNotes>Initial release</PackageReleaseNotes>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Version>2.0.0-preview3</Version>
    </PropertyGroup>

</Project>
